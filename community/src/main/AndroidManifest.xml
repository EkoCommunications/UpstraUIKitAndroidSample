<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amity.community">

    <application
        android:name="com.amity.community.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UpstraUIKitAndroidSample">

        <activity android:name="com.amity.community.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.amity.community.newsfeed.CreatePostActivity"
            android:theme="@style/Amity.Base.Theme.AmityApp" />
        <activity android:name="com.amity.community.newsfeed.NewsFeedHomeActivity" />
        <activity android:name="com.amity.community.homepage.MyCustomCommunityHomePageActivity" />
        <activity android:name="com.amity.community.FeatureListActivity" />
    </application>

</manifest>